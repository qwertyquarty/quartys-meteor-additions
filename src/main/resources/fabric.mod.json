{
  "schemaVersion": 1,
  "id": "quartys-meteor-additions",
  "version": "${version}",
  "name": "Quarty's Meteor Additions",
  "description": "Meteor addon with useful stuff.",
  "authors": [
    "qwertyquarty",
    "EdwiP"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "de.qwqu.qma.Addon"
    ]
  },
  "mixins": [
    "qma.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.21.4",
    "meteor-client": "*"
  }
}
