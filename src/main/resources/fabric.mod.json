{
  "schemaVersion": 1,
  "id": "quartys-meteor-additions",
  "version": "${version}",
  "name": "Addon Template",
  "description": "An addon template for Meteor Client addons.",
  "authors": ["qwertyquarty"],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": ["de.qwqu.qma.Addon"]
  },
  "mixins": ["qma.mixins.json"],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
